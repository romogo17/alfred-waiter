{
  "expo": {
    "name": "Alfred Waiter",
    "description": "An app to order foor from within a restaurant.",
    "slug": "alfred-waiter",
    "privacy": "public",
    "sdkVersion": "30.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.una.alfredwaiter",
      "supportsTablet": true
    },
    "android": {
      "package": "com.una.alfredwaiter"
    }
  }
}
